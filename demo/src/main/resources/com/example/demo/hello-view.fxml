<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" cache="true" prefHeight="175.0" prefWidth="416.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="10.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label fx:id="lblMessage" text="Message" textAlignment="CENTER" textFill="#ff0707" />
      <Button fx:id="btnClick" mnemonicParsing="false" onAction="#clickedButton" text="Button" />
   </children>
</VBox>
